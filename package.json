{
  "name": "react-oauth2",
  "version": "1.1.2",
  "description": "A social oAUth Sign-in / Log-in Component for React. You can login via Facebook, Google, Twitter",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=17.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxifjaved/react-oauth2.git"
  },
  "author": "Muhammad Asif Javed <axif.javed@gmail.com>",
  "bugs": {
    "url": "https://github.com/maxifjaved/react-oauth2/issues"
  },
  "homepage": "https://github.com/maxifjaved/react-oauth2#readme",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/react-oauth2.esm.js",
  "size-limit": [
    {
      "path": "dist/react-oauth2.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-oauth2.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/create-react-class": "^15.6.3",
    "@types/object-keys": "^1.0.1",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.14",
    "@types/request": "^2.48.8",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "create-react-class": "^15.7.0",
    "moment": "^2.29.1",
    "object-keys": "^1.1.1",
    "query-string": "^7.1.1",
    "request": "^2.88.2",
    "url": "^0.11.0"
  }
}
